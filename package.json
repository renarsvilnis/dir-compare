{
  "name": "@renarsvilnis/dir-compare",
  "description": "Utility for comparing directories",
  "version": "1.0.4",
  "author": "RenÄrs Vilnis",
  "keywords": [
    "compare",
    "directory",
    "folder"
  ],
  "engines": {
    "node": ">=11.11.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/renarsvilnis/dir-compare"
  },
  "bugs": {
    "url": "https://github.com/renarsvilnis/dir-compare/issues"
  },
  "main": "src/index.ts",
  "mainn": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src/**/*",
    "package-lock.json",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "buffer-equal": "1.0.0",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@types/buffer-equal": "0.0.30",
    "@types/deasync": "0.1.0",
    "@types/jest": "24.0.23",
    "@types/minimatch": "3.0.3",
    "@types/node": "12.12.20",
    "@types/tar-fs": "1.16.1",
    "@types/temp": "0.8.34",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "common-tags": "1.8.0",
    "deasync": "0.1.16",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "24.9.0",
    "jest-each": "24.9.0",
    "prettier": "1.19.1",
    "tar-fs": "2.0.0",
    "temp": "0.9.1",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.3"
  },
  "scripts": {
    "pretest": "tsc -p tsconfig.json",
    "test": "jest ./src --collect-coverage",
    "test:watch": "jest ./src --watch",
    "test:alt": "ts-node tests/runTests.ts",
    "test:old": "istanbul cover tests/runTests.ts",
    "test:run": "npx jest ./tests/runTests.test.ts --watch",
    "build:ts": "tsc",
    "build:clean": "rm -rf ./lib",
    "build": "npm run build:clean && npm run build:ts",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build:ts -- --noEmit",
    "lint:ts": "npm run build:ts -- --noEmit",
    "lint:eslint": "eslint src/**/* ",
    "fix": "tsc --noEmit && eslint 'src/**/*.{js,ts,ts}' --fix"
  }
}
